/*
Selection sort algorithm:
  for i from 1 to n:
    minIndex <- i
    for j from i + i to n:
      if A[i] < A[minIndex]:
         minIndex <= j
    {A[minIndex] = min A[i...n]}
    swap(A[i], A[minIndex])
    {A[1...i] is in final position}

The complexity for this sort algorithm is O(n^2)
 */

#include <iostream>
#include <vector>

usign namespace std;

int main()
{
  vector<int> A = {100, 5 , 40, 30 , 20 , 10 , 78, 9, 0, -10];
  
  return 0;
}
